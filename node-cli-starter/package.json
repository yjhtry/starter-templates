{
  "name": "@yjhtry/node-cli-starter",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.1",
  "description": "starter templates",
  "license": "MIT",
  "homepage": "https://github.com/yjhtry/starter-templates/node-cli-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjhtry/starter-templates.git"
  },
  "bugs": {
    "url": "https://github.com/yjhtry/starter-templates/issues"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "hello": "bin/hello.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "dev": "cross-env DEBUG=create-all esno ./src/cli.ts",
    "start": "esno ./src/cli.ts",
    "build": "unbuild",
    "typecheck": "tsc",
    "prepublishOnly": "nr build",
    "release": "pnpm build && bumpp && pnpm publish --no-git-checks",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@npmcli/config": "^8.1.0",
    "cli-progress": "^3.12.0",
    "deepmerge": "^4.3.1",
    "execa": "^8.0.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@antfu/utils": "^0.7.7",
    "@types/cli-progress": "^3.11.5",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.11.16",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "bumpp": "^9.3.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.2.2"
  }
}
