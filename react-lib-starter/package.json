{
  "name": "package-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.1",
  "description": "react component library",
  "license": "MIT",
  "homepage": "https://github.com/yourname/package-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/package-name.git"
  },
  "bugs": {
    "url": "https://github.com/yjhtry/yourname/package-name/issues"
  },
  "scripts": {
